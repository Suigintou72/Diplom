<Window x:Class="Diplom.Games.ListOther"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom.Games"
        mc:Ignorable="d"
        Height="133.867" Width="275.2" WindowStyle="ToolWindow">
    <Window.Background>
        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}"/>
    </Window.Background>
    <Grid>
        <Button Content="Тетрис" Width="215" FontSize="14" BorderBrush="#FF74A5B1" Click="Button_Click_1" Margin="10,10,9.6,51.4" Cursor="Hand">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
            </Button.Background>
        </Button>
        <Button Content="Змейка" Width="215" FontSize="14" BorderBrush="#FF74A5B1" Click="Button_Click" Margin="10,51,9.6,10.4" Cursor="Hand">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.GradientActiveCaptionColorKey}}"/>
            </Button.Background>
        </Button>
    </Grid>
</Window>
